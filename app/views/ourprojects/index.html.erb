<% if notice %>
<p id="notice"><%= notice %></p>
<% end %>
<h1 id="projecth1">Проекты</h1>
<% @ourprojects.each do |ourproject| %>
  <div class="project">
    <div class="project_title"><%= ourproject.title %></div>
    <div class="project_url"><%= link_to slice_http(ourproject.url), ourproject.url %></div>
    <div class="project_description"><%= ourproject.description %></div>
    <div class="project_actions">
      <%= link_to 'Показать', ourproject %> | 
      <%= link_to 'Редактировать', edit_ourproject_path(ourproject) %> | 
      <%= link_to 'Удалить', ourproject, :confirm => 'Вы уверены?', :method => :delete %>
    </div>
  </div>
<% end %>
<%= button_to 'Новый проект', new_ourproject_path, :method => :get %>
<div class="paginate">
  <%= will_paginate(@ourprojects) %>
</div>

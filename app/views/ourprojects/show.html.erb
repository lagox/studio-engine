<% if notice %>
<p id="notice"><%= notice %></p>
<% end %>
<h1 id="projecth1" ><%= @ourproject.title %></h1>
<div class="project">  
  <div class="project_url"><%= link_to slice_http(@ourproject.url), @ourproject.url %></div>
  <div class="project_description"><%= @ourproject.description %></div>
  <div class="project_actions">
    <%= link_to 'Редактировать', edit_ourproject_path(@ourproject) %> | 
    <%= link_to 'Назад', ourprojects_path %>
  </div>
</div>
